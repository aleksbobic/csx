    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "html2canvas": "*",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mobx": "^6.6.1",
    "mobx-react": "^7.5.2",
