      "arrow-parens": [
        2,
        "as-needed"
      ]
    }
  },
  "proxy": "http://localhost:3000",
  "prettier": {
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
